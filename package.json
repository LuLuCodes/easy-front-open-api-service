{
  "name": "easy-front-open-api-service",
  "version": "1.0.0",
  "description": "基于nestjs的Open API Service框架",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "bootstrap": "pnpm install",
    "prebuild": "rimraf api",
    "build": "rimraf api && nest build && shx cp ./src/.env.production ./api/.env",
    "build:prod": "rimraf api && nest build && shx cp ./src/.env.production ./api/.env",
    "build:pre-release": "rimraf api && nest build && shx cp ./src/.env.pre-release ./api/.env",
    "build:test": "rimraf api && nest build && shx cp ./src/.env.test ./api/.env",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "rimraf api && cross-env NODE_ENV=development nest start --watch",
    "start:debug": "rimraf api && cross-env NODE_ENV=development nest start --debug --watch",
    "start:prod": "node api/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "db": "node db-tools/index.js",
    "cz": "git-cz"
  },
  "dependencies": {
    "@liaoliaots/nestjs-redis": "^3.1.1",
    "@nestjs/axios": "^0.0.3",
    "@nestjs/bull": "^0.4.2",
    "@nestjs/common": "^8.0.0",
    "@nestjs/config": "^1.0.1",
    "@nestjs/core": "^8.0.0",
    "@nestjs/microservices": "^8.0.6",
    "@nestjs/platform-express": "^8.0.0",
    "@nestjs/schedule": "^1.0.1",
    "@nestjs/sequelize": "^8.0.0",
    "@nestjs/serve-static": "^2.2.2",
    "@nestjs/swagger": "^5.0.9",
    "ali-oss": "^6.16.0",
    "amqp-connection-manager": "^3.7.0",
    "amqplib": "^0.8.0",
    "body-parser": "^1.19.0",
    "bull": "^3.29.3",
    "chalk": "^4.1.2",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "compression": "^1.7.4",
    "connect-redis": "^6.0.0",
    "cookie-parser": "^1.4.5",
    "cross-env": "^7.0.3",
    "crypto-js": "^4.1.1",
    "dayjs": "^1.10.7",
    "dotenv": "^10.0.0",
    "@easy-front-core-sdk/alipay": "^1.2.14",
    "@easy-front-core-sdk/kits": "^1.2.14",
    "@easy-front-core-sdk/miniprogram": "^1.2.14",
    "@easy-front-core-sdk/wx": "^1.2.14",
    "@easy-front-core-sdk/wxpay": "^1.2.14",
    "easy-front-core-sdk": "^1.2.14",
    "express-rate-limit": "^5.3.0",
    "express-session": "^1.17.2",
    "fs-extra": "^10.0.0",
    "helmet": "^4.6.0",
    "ioredis": "^4.27.9",
    "lodash": "^4.17.21",
    "log4js": "^6.3.0",
    "mysql2": "^2.3.0",
    "nestjs-ali-oss": "^1.0.0",
    "redis": "^3.1.2",
    "redlock": "^4.2.0",
    "reflect-metadata": "^0.1.13",
    "request-ip": "^2.1.3",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "sequelize": "6.6.2",
    "sequelize-typescript": "^2.1.0",
    "sequelize-typescript-generator": "^6.1.1",
    "stacktrace-js": "^2.0.2",
    "swagger-ui-express": "^4.1.6",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@nestjs/cli": "^8.0.0",
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^8.0.0",
    "@types/ali-oss": "^6.0.11",
    "@types/body-parser": "^1.19.1",
    "@types/bull": "^3.15.5",
    "@types/compression": "^1.7.2",
    "@types/cookie-parser": "^1.4.2",
    "@types/crypto-js": "^4.0.2",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.13",
    "@types/fs-extra": "^9.0.13",
    "@types/ioredis": "^4.27.4",
    "@types/jest": "^27.0.1",
    "@types/lodash": "^4.14.173",
    "@types/node": "^16.0.0",
    "@types/request-ip": "^0.0.37",
    "@types/sequelize": "^4.28.10",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^4.28.2",
    "@typescript-eslint/parser": "^4.28.2",
    "commitizen": "^4.2.4",
    "commitlint-config-cz": "^0.13.2",
    "cz-conventional-changelog": "^3.3.0",
    "cz-customizable": "^6.3.0",
    "eslint": "^7.30.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "husky": "^7.0.2",
    "jest": "^27.0.6",
    "lint-staged": "^11.1.2",
    "prettier": "^2.4.1",
    "sequelize-auto": "^0.8.4",
    "shx": "^0.3.3",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.3.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.js": [
      "standard --fix",
      "git add"
    ]
  }
}
